         MVC   TSRAWRK,TSRAWRK2    SAVED DEBIT                                  
         ZAP   TSRACR,=P'0'                                                     
         MVC   TSRKCDA,AUTTODAY                                                 
         MVI   TSRKTYP,TTREPQ                                                   
         MVI   TSRFLAG,TSRFLGNC                                                 
         XC    TSRKBNO,TSRKBNO                                                  
         GOTO1 ATSAR,DMCB,(RC),TSAADD,1                                         
         CLI   BCTSERRS,0                                                       
         BE    *+6                                                              
         DC    H'0'                                                             
