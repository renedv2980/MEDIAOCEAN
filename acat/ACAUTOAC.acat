**********************************************************************          
* READ SR PASSIVE PNTRS AND BUILD TABLE BY REFERENCE NUMBER          *          
* APPLY PERCENT TO EACH DEBIT ENTRY BY BILL NUMBER/EST/MOS           *          
**********************************************************************          
         USING AARPKEY,R4                                                       
*                                                                               
SRPNTRTB NTR1  BASE=*,LABEL=*                                                   
         XC    SVKEY,SVKEY         BUILD KEY TO READ SR RECDS                   
*                                                                               
         GOTO1 ATSAR,DMCB,(RC),TSAINI,1 REINIT FIRST BUFFER                     
         BE    *+6                                                              
         DC    H'0'                                                             
*                                                                               
SRPNT10  DS    0H                                                               
         LA    R4,SVKEY                                                         
         XC    AARPKEY,AARPKEY     INIT KEY                                     
         MVI   AARPTYP,AARPTYPQ    AA RCV PASSIVE POINTER                       
         MVI   AARPSUB,AARPSUBQ    AA RCV PASSIVE POINTER SUB TYPE              
         MVC   AARPCPY,AUTCPY      COMPANY                                      
         MVI   AARPIND,AARPIEST    ESTIMATE CODE POINTER                        
*                                                                               
         GOTO1 ADMRDHI,DMCB,(RC)                                                
         B     SRPNT30                                                          
*                                                                               
SRPNT20  GOTO1 ADMRDSEQ,DMCB,(RC)    READ NEXT SR POINTER                       
*                                                                               
SRPNT30  DS    0H                                                               
         MVI   SKIPCHK,C'Y'        NEW PP - INDICATE NEED SKIP CHECK            
*                                                                               
         CLC   SVKEY(AARPCLT-AARPKEY),IOKEY                                     
         BNE   SRPNT210                                                         
*                                                                               
         CLI   AUTSYSTM,C' '                                                    
         BNH   *+14                                                             
         CLC   IOKEY+AARPSYS-AARPKEY(L'AARPSYS),AUTSYSTM                        
         BNE   SRPNT20                                                          
*                                                                               
         OC    AUTALST,AUTALST     DO WE HAVE A CLIENT LIST FROM REQST          
         BZ    SRPNT33                                                          
*                                                                               
         MVC   CLNT,IOKEY+AARPCLT-AARPKEY                                       
         BRAS  RE,CHKCLT           CHECK IF IT IS A VALID CLIENT                
         BNE   SRPNT20             READ NEXT TRANSACTION                        
         B     SRPNT35             MATCHING CLIENT FOUND                        
*                                                                               
SRPNT33  DS    0H                                                               
         CLC   AUTCLNT,SPACES                                                   
         BNH   *+14                                                             
         CLC   IOKEY+AARPCLT-AARPKEY(L'AARPCLT),AUTCLNT                         
         BNE   SRPNT20                                                          
*                                                                               
SRPNT35  DS    0H                                                               
         MVC   SVSRPKEY,IOKEY      SAVE SR POINTER KEY                          
*                                                                               
         GOTO1 ADMGET,DMCB,(RC),0 GET RECORD                                    
         BE    *+6                                                              
         DC    H'0'                                                             
*                                                                               
         CLC   IOKEY+1(2),=C'SR'                                                
         BNE   SRPNT20                                                          
*                                                                               
         CLI   IOKEY+(L'IOKEY-1),X'00'  DEBIT?                                  
         BE    SRPNT60                                                          
*                                                                               
         MVC   SVKEY,IOKEY                                                      
         MVI   SVKEY+(L'SVKEY-1),X'00'  MAY NOT BE THERE                        
         GOTO1 ADMRDHI,DMCB,(RC)        READ HIGH SEQUENCE 00                   
*        BE    SRPNT50                                                          
         CLC   SVKEY(L'SVKEY-1),IOKEY                                           
         BE    SRPNT50                                                          
         DC    H'0'                                                             
*                                                                               
SRPNT40  GOTO1 ADMRDSEQ,DMCB,(RC)                                               
         CLC   SVKEY(L'TRNKEY-1),IOKEY                                          
         BNE   SRPNT160                                                         
*                                                                               
SRPNT50  GOTO1 ADMGET,DMCB,(RC),0 GET RECORD                                    
         BE    *+6                                                              
         DC    H'0'                                                             
*                                                                               
         CLC   IOKEY+1(2),=C'SR'                                                
         BNE   SRPNT20                                                          
*                                                                               
SRPNT60  DS    0H                                                               
         LA    R2,TSRAWRK                                                       
         USING TSRAD,R2                                                         
         LA    R4,IO                                                            
         USING TRNRECD,R4                                                       
*                                                                               
         CLC   AUTACCT,SPACES                                                   
         BNH   *+14                                                             
         CLC   TRNKACT,AUTACCT                                                  
         BNE   SRPNT150                                                         
*                                                                               
         XC    TSRAWRK,TSRAWRK     INITIAZIZE WORK AREA                         
         ZAP   SVTRNAMT,=P'0'                                                   
         ZAP   SVCOMAMT,=P'0'                                                   
         MVI   SVBILTYP,X'00'                                                   
         XC    SVCLNET,SVCLNET                                                  
*                                                                               
         MVC   TSRKBNO,TRNKREF         SR TRANSACTION NUMBER                    
         MVC   TSRKBDA,TRNKDATE        SR TRANSACTION DATE                      
*                                                                               
         LR    R5,R4                                                            
         AH    R5,=Y(TRNRFST-TRNKEY)                                            
*                                                                               
SRPNT70  CLI   0(R5),0                                                          
         BE    SRPNT140                                                         
*                                                                               
         CLI   0(R5),TRNELQ        IS IT X'44' ELEMENT                          
         BE    SRPNT90                                                          
*                                                                               
         CLI   0(R5),TRSELQ        IS IT X'60' ELEMENT                          
         BE    SRPNT100                                                         
*                                                                               
         CLI   0(R5),MDTELQ        IS IT X'1A' ELEMENT                          
         BE    SRPNT110                                                         
*                                                                               
         CLI   0(R5),MDPELQ        IS IT X'6A' ELEMENT                          
         BE    SRPNT110            CLI/PRO/EST/MOS IN X'6A'=X'1A'               
*                                                                               
         CLI   0(R5),MBIELQ        IS IT X'F3' ELEMENT                          
         BE    SRPNT130                                                         
*                                                                               
         CLI   0(R5),RALELQ        IS IT X'D9' ELEMENT                          
         BE    SRPNT135                                                         
*                                                                               
         CLI   0(R5),FFTELQ        IS IT X'DB' ELEMENT                          
         BE    SRPNT136                                                         
*                                                                               
SRPNT80  SR    R1,R1                                                            
         IC    R1,1(R5)            GET LENGTH OF ELEMENT                        
         AR    R5,R1                                                            
         B     SRPNT70                                                          
*                                                                               
         USING TRNELD,R5                                                        
SRPNT90  DS     0H                                                              
         TM    TRNSTAT,TRNSDR      IS IT A DEBIT                                
         BO    *+8                                                              
         OI    TSRFLAG,TSRFLGCR                                                 
*                                                                               
         ZAP   SVTRNAMT,TRNAMNT                                                 
         B     SRPNT80                                                          
*                                                                               
         USING TRSELD,R5                                                        
SRPNT100 DS    0H                                                               
*        TM    TRSSTAT,TRSSOFFS    IS IT AN OFFSET                              
*        BO    SRPNT150            SKIP OFFSETS                                 
         B     SRPNT80                                                          
         DROP  R5                                                               
*                                                                               
         USING MDTELD,R5           PROCESS 1A OR 6A                             
SRPNT110 DS    0H                                                               
         CLI   MDTSYS,C'J'                                                      
         BE    SRPNT150            SKIP THIS TRANSACTION                        
*                                                                               
*        TM    TSRFLAG,TSRFLGCR                                                 
*        BO    SRPNT112                                                         
*        CLC   SVSRPKEY+AARPEST-AARPKEY(L'AARPEST),SPACES                       
*        BNH   SRPNT112                                                         
*        CLC   SVSRPKEY+AARPEST-AARPKEY(L'MDTEST),MDTEST                        
*        BNE   SRPNT150            SKIP THIS TRANSACTION                        
*                                                                               
SRPNT112 DS    0H                                                               
         MVC   SVSYS,MDTSYS        SYSTEM                                       
         MVC   SVMED,MDTMED        MEDIA                                        
         GOTOR GETSYSTM            GET SYSTEM BASED ON MEDIA                    
         MVC   TSRKSYS,SVSYS                                                    
*                                                                               
         CLI   AUTMEDIA,C' '                                                    
         BNH   *+14                                                             
         CLC   SVMED,AUTMEDIA                                                   
         BNE   SRPNT150                                                         
*                                                                               
         MVC   TSRKMED,MDTMED                                                   
*                                                                               
         CLC   AUTCLNT,SPACES                                                   
         BNH   *+14                                                             
         CLC   MDTCLI,AUTCLNT                                                   
         BNE   SRPNT150            WRONG CLIENT                                 
*                                                                               
         MVC   TSRKCLI,MDTCLI          MOVE IN CLIENT CODE TO WORK              
         MVC   SVCLI,MDTCLI                                                     
*                                                                               
         CLI   MDTMOS+1,X'13'      IS THIS THE 13TH MONTH                       
         BE    SRPNT150            SKIP THIS TRANSACTION                        
         MVC   SVMOS(L'MDTMOS),MDTMOS                                           
         GOTOR GTINSRTN            GET INSERTION DATE                           
*                                                                               
         BRAS  RE,FIXPMOS                                                       
         MVC   TSRKMOS,SVMOS                                                    
*                                                                               
SRPNT115 DS    0H                                                               
         CLI   0(R5),X'6A'                                                      
         BE    SRPNT120                                                         
         ICM   R1,15,MDTCOM        GET NET/COMMISSION AMOUNT                    
         CVD   R1,DUB                                                           
         SP    SVTRNAMT,DUB        SUBTRACT NET FROM GROSS                      
         ZAP   SVCOMAMT,DUB                                                     
         B     SRPNT140                                                         
*                                                                               
         USING MDPELD,R5                                                        
SRPNT120 SP    SVTRNAMT,MDPCOM                                                  
         ZAP   SVCOMAMT,MDPCOM                                                  
         B     SRPNT140                                                         
*                                                                               
         USING MBIELD,R5           PROCESS X'F3'                                
SRPNT130 DS    0H                                                               
*        TM    TSRFLAG,TSRFLGCR                                                 
*        BO    SRPNT132                                                         
*        CLC   SVSRPKEY+AARPEST-AARPKEY(L'AARPEST),SPACES                       
*        BNH   *+14                                                             
*        CLC   SVSRPKEY+AARPEST-AARPKEY(L'MBIEST),MBIEST                        
*        BNE   SRPNT150            SKIP THIS TRANSACTION                        
*                                                                               
SRPNT132 DS    0H                                                               
         MVC   SVSYS,MBISYS        SYSTEM                                       
         MVC   SVMED,MBIMED        MEDIA                                        
*                                                                               
         CLI   AUTMEDIA,C' '                                                    
         BNH   *+14                                                             
         CLC   SVMED,AUTMEDIA                                                   
         BNE   SRPNT150                                                         
*                                                                               
         GOTOR GETSYSTM            GET SYSTEM BASED ON MEDIA                    
         MVC   TSRKSYS,SVSYS           PUT SYSTEM IN TABLE                      
*                                                                               
         CLI   TSRKSYS,C'N'            IS THIS NET SYSTEM ?                     
         BNE   *+12                                                             
         CLI   AUTPRF2,C'S'        RUN BY SYSTEM OR SYS/MED ?                   
         BE    *+10                                                             
         MVC   TSRKMED,SVMED           PUT MEDIA CODE IN TABLE                  
*                                                                               
         CLC   AUTCLNT,SPACES                                                   
         BNH   *+14                                                             
         CLC   MBICLI,AUTCLNT                                                   
         BNE   SRPNT150            WRONG CLIENT                                 
*                                                                               
         MVC   TSRKCLI,MBICLI          MOVE IN CLIENT CODE TO WORK              
         MVC   SVCLI,MBICLI                                                     
*                                                                               
         CLI   MBIMOS+1,X'13'      IS THIS THE 13TH MONTH                       
         BE    SRPNT150            SKIP THIS TRANSACTION                        
         MVC   SVMOS(L'MBIMOS),MBIMOS                                           
         GOTOR GTINSRTN            GET INSERTION DATE                           
*                                                                               
         BRAS  RE,FIXPMOS                                                       
*                                                                               
         MVC   TSRKMOS,SVMOS           MOVE IN MONTH OF SERVICE                 
         B     SRPNT80                                                          
*                                                                               
         USING RALELD,R5           PROCESS X'D9'                                
SRPNT135 DS    0H                                                               
         CLI   RALTYPE,RALTOFS                                                  
         BNE   *+14                SKIP OFFSETS                                 
         MVC   TSRKCDA,RALODAT                                                  
         B     SRPNT80                                                          
*                                                                               
         MVC   TSRKCDA,RALADAT                                                  
         B     SRPNT80                                                          
*                                                                               
         USING FFTELD,R5           PROCESS X'DB'                                
SRPNT136 DS    0H                                                               
         CLI   FFTTYPE,FFTTCNET    CALCULATED NET                               
         BNE   *+14                                                             
         MVC   SVCLNET,FFTDATA                                                  
         B     SRPNT80                                                          
*                                                                               
         CLI   FFTTYPE,FFTTMXTY    MX ELEMENT AND BILL TYPES                    
         BNE   SRPNT80                                                          
*                                                                               
         LA    RE,XBILTYPS                                                      
SRPNT137 CLI   0(RE),X'FF'                                                      
         BE    SRPNT80                                                          
         LLC   RF,0(RE)            FFTELD ELEM DATA LENGTH                      
         BCTR  RF,0                                                             
         EX    RF,*+8                                                           
         B     *+10                                                             
         CLC   1(0,RE),FFTMXTB     SAME BILL TYPE AS IN FFTELD?                 
