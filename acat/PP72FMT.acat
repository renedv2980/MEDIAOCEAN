FBEDL    DS    0H                                                               
         CLI   PBDUIND,X'89'                                                    
         BNE   FBEDL5                                                           
         EDIT  (P8,DUB),(10,(R4)),2,ALIGN=LEFT                                  
         BR    RE                                                               
FBEDL5   EDIT  (P8,DUB),(10,(R4)),ALIGN=LEFT                                    
         BR    RE                                                               
