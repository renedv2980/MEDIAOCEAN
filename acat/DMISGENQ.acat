         LARL  R3,ALPHANUM                                                      
INIT51   LHI   R1,MAXFEXQ          # of file exclusive enqueues                 
         LARL  R2,ALPHANUM                                                      
INIT52   XC    TOKEND(TOKNLNQ),TOKEND                                           
         MVC   TOKNEYE,=CL8'WKF?TKN?'                                           
         MVC   TOKNEYE+3(1),0(R3)                                               
         MVC   TOKNEYE+7(1),0(R2)                                               
         ZAP   TOKNREQ#,=P'0'                                                   
         ZAP   TOKNENQ#,=P'0'                                                   
         ZAP   TOKNWT#,=P'0'                                                    
         ZAP   TOKNDEQ#,=P'0'                                                   
         LA    RE,TOKNLNQ(,RE)                                                  
         LA    R2,1(,R2)                                                        
         JCT   R1,INIT52                                                        
