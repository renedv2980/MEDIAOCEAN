         CLI   ANS,C'C'            CC=HIGH FOR CANCEL                           
         BE    GETMSGX                                                          
         LNR   R0,R0                                                            
         CLI   ANS,C'R'            CC=LOW FOR RETRY                             
         BE    GETMSGX                                                          
         CLC   MSGOPT3,SPACES      TEST IF OPTION3 ALLOWED                      
         BE    GETMSG1                                                          
         SR    R0,R0                                                            
         CLC   ANS(1),MSGOPT3      CC=EQL FOR IGNORE/DEQUEUE                    
         BE    GETMSGX                                                          
         B     GETMSG1                                                          
