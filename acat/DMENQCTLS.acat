GETMSG   TM    PROCMODE,OFFLINE    SEND MSG TO CONSOLE AND GET ANS              
         BO    GETMSG0                                                          
         GOTO1 VTICTOC,DUB,C'SSET'                                              
GETMSG0  XC    CCB(16),CCB                                                      
         MVI   CCB+7,04                                                         
         LA    R1,MSG              BUILD WRITE CCW                              
         STCM  R1,7,GETMSGC1+1                                                  
         LA    R1,L'MSG                                                         
         STH   R1,GETMSGC1+6                                                    
         LA    R1,ANS              BUILD READ CCW                               
         STCM  R1,7,GETMSGC2+1                                                  
         LA    R1,1                                                             
         STH   R1,GETMSGC2+6                                                    
*                                                                               
GETMSG1  LA    R1,CCB                                                           
         MVI   ANS,C' '                                                         
         LA    R0,GETMSGC1                                                      
         ST    R0,CCB+8                                                         
         EXCP  (1)                                                              
         WAIT  (1)                                                              
         LA    R0,GETMSGC2                                                      
         ST    R0,CCB+8                                                         
         EXCP  (1)                                                              
         WAIT  (1)                                                              
         OI    ANS,X'40'                                                        
*                                                                               
GETMSG2  LA    R0,1                                                             
         CLI   ANS,C'C'            CC=HIGH FOR CANCEL                           
         BE    GETMSGX                                                          
         LNR   R0,R0                                                            
         CLI   ANS,C'R'            CC=LOW FOR RETRY                             
         BE    GETMSGX                                                          
         CLC   MSGOPT3,SPACES      TEST IF OPTION3 ALLOWED                      
         BE    GETMSG1                                                          
         SR    R0,R0                                                            
         CLC   ANS(1),MSGOPT3      CC=EQL FOR IGNORE/DEQUEUE                    
         BE    GETMSGX                                                          
         B     GETMSG1                                                          
*                                                                               
         DC    0D'0'                                                            
GETMSGC1 DC    X'0900000000000000' WRITE TO CONSOLE                             
GETMSGC2 DC    X'0A00000000000000' READ FROM CONSOLE                            
*                                                                               
GETMSGX  TM    PROCMODE,OFFLINE                                                 
         BO    GETMSGY                                                          
         GOTO1 VTICTOC,DUB,C'RSET'                                              
GETMSGY  LTR   R0,R0               EXIT WITH CC LOW=R,EQL=I,HIGH=C              
         BR    R9                                                               
         SPACE 2                                                                
GETTIM   GETIME BINARY             GET TIME IN SECS INTO TIME                   
         ST    R1,TIME                                                          
         BR    R9                                                               
         SPACE 2                                                                
GETSUB   L     RF,VENQFIL          GET FILE SUBID INTO DUB                      
