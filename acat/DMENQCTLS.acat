         CVD   R0,DUB                                                           
         OI    DUB+7,X'0F'                                                      
         UNPK  DUB(4),DUB+5(3)                                                  
         BR    R9                                                               
         SPACE 2                                                                
SETTIM   TM    PROCMODE,OFFLINE    WAIT FOR TIME SECONDS                        
         BO    SETTIM1                                                          
         GOTO1 VTICTOC,DUB,C'SSET'                                              
SETTIM1  L     R1,TIME                                                          
         LA    R3,ECB                                                           
         XC    0(4,R3),0(R3)                                                    
         SETIME (1),(3)                                                         
         LA    R1,ECB                                                           
         WAIT  (1)                                                              
         TM    PROCMODE,OFFLINE                                                 
         BOR   R9                                                               
         GOTO1 VTICTOC,DUB,C'RSET'                                              
         BR    R9                                                               
         SPACE 2                                                                
ERR      ICM   R9,8,RSLT           FATAL ERROR EXIT                             
         ST    R9,DUB                                                           
         DC    H'0'                                                             
         DC    C'DMENQDEQ FATAL ERROR'                                          
         DC    0H'0'                                                            
         SPACE 2                                                                
EXCP     MVI   IOERR,0             EXECUTE CHANNEL PROGRAM AT R1                
         TM    PROCMODE,OFFLINE                                                 
         BO    EXCP1                                                            
         LR    R0,R1                                                            
         GOTO1 VTICTOC,DUB,C'SSET'                                              
         LR    R1,R0                                                            
EXCP1    XC    CCB(16),CCB                                                      
         L     RF,VFILE                                                         
         MVC   CCB+6(2),6(RF)                                                   
         OI    CCB+2,X'10'                                                      
         ST    R1,CCB+8            SET A(CCW CHAIN TO BE EXECUTED)              
         LA    R1,CCB                                                           
*                                                                               
         EXCP  (1)                 INITIATE I/O                                 
         WAIT  (1)                                                              
         L     R1,CCB+12           RETURN R1=A(LAST CCW EXECUTED)               
         SH    R1,=H'8'                                                         
*                                                                               
         TM    CCB+2,X'20'         TEST UNRECV ERROR                            
         BZ    *+12                                                             
         OI    IOERR,X'80'                                                      
         B     EXCPX                                                            
         TM    CCB+3,X'0C'         TEST NO REC FOUND                            
         BZ    *+12                                                             
         OI    IOERR,X'40'                                                      
         B     EXCPX                                                            
         TM    CCB+4,X'02'         TEST UNIT CHECK                              
         BZ    *+12                                                             
         OI    IOERR,X'80'                                                      
         B     EXCPX                                                            
         TM    CCB+5,X'7F'         TEST OTHER ERRORS                            
         BZ    *+12                                                             
         OI    IOERR,X'80'                                                      
         B     EXCPX                                                            
*                                                                               
EXCPX    TM    PROCMODE,OFFLINE                                                 
         BO    EXCPY                                                            
         LR    R0,R1                                                            
         GOTO1 VTICTOC,DUB,C'RSET'                                              
         LR    R1,R0                                                            
EXCPY    CLI   IOERR,0                                                          
         BR    R9                  EXIT WITH CC EQL IF OK                       
*&&                                                                             
         EJECT                                                                  
