*+++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++*             
* NOTE TO THE CURIOUS -- THE FIRST BYTE OF TSVCREQ IS SET BY SOME *             
* OF THE SERVICE REQUESTS TO X'02' TO SUPPRESS THE FEATURE THAT   *             
* IGNORES ALL INPUT FIELDS THAT FOLLOW THE CURSOR, LIKE =PQ       *             
*+++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++*             
*                                                                               
         TITLE 'TISTR - STEREO 3270 TERMINAL INPUT TRANSLATOR'                  
*        THIS MODULE CONVERTS A STEREO 3270 INPUT DATA STRING                   
*        IN THE TIA TO STANDARD INTERNAL FORMAT IE TRANSLATES                   
*        STEREO DATA BUFFER FIELDS TO (LEN)(ABS#1)(ABS#2)(DATA)                 
*        AND MERGES THESE FLDS TO THE TWA. PARAMS VIA R1                        
*                                                                               
*        AL4   A(TIA)                                                           
*        AL4   A(TWA)                                                           
*        AL4   A(UTL)                                                           
*                                                                               
*        THE PARAM LIST IS OVERWRITTEN WITH RETURN VALUES                       
*                                                                               
*        XL2   TWA INDEX TO FIRST INPUT FLD                                     
*        XL2   TWA INDEX TO LAST INPUT FLD                                      
*        XL2   NUMBER OF INPUT FLDS (SET TO ZERO IF MERGE ERRORS)               
*        XL1   AID VALUE X'00'=ENTER,X'01-24'=PF KEY NUMBER                     
*        XL1   N/D                                                              
*        XL2   CURSOR SCREEN ADDRESS                                            
*        XL2   DISP TO TWA FIELD THAT CURSOR IS IN OR                           
*              ZERO IF CURSOR NOT POSITIONED TO A FORMATTED FIELD               
*        XL1   INDEX TO CURSOR POSITION WITHIN TWA FIELD                        
*        XL2   DISP TO FIRST HELP SUPPORTED FIELD CONTAINING '?'                
*        XL1   N/D                                                              
         SPACE 1                                                                
         PRINT NOGEN                                                            
TISTR    CSECT                                                                  
*                                                                               
         NMOD1 FLDMRGX-TIOBD,**TISTR*                                           
         USING TIOBD,RC                                                         
         ST    R1,FMRGSR1          SAVE R1                                      
         MVC   FMRGTIAA(12),0(R1)  SAVE A(TIA) & A(TWA) & A(UTL)                
         XC    TIOBD(TIOBL),TIOBD                                               
*                                                                               
INIT1    L     RF,FMRGUTLA         RF=A(UTL ENTRY)                              
         USING UTLD,RF                                                          
         MVC   SVCRSAVE,TSVCREQ    SAVE SERVICE REQUEST VALUES                  
         MVC   CTRY,TCTRY          EXTRACT COUNTRY/LANGUAGE/SYSTEM              
         MVC   LANG,TLANG                                                       
         MVC   SYS,TOVSYS                                                       
         MVC   SVCREQ,TSVCREQ+1                                                 
         MVI   SAVIND2,0           EXTRACT PGMIND2 FROM PROGRAM LIST            
         XR    RE,RE                                                            
         ICM   RE,7,TAPRG                                                       
         BZ    INIT1B                                                           
         L     R6,=V(SSB)          TAPRG IS DISPLACEMENT?                       
         TM    SSBSTAT4-SSBD(R6),SSBPGDSP                                       
         BZ    INIT1A              NO                                           
*                                                                               
         XR    R6,R6               INDEX INTO TAPRG                             
         ICM   R6,7,TARSYS                                                      
         ICM   R6,15,SEPGMS-SELISTD(R6)                                         
         AR    RE,R6                                                            
*                                                                               
INIT1A   MVC   SAVIND2,PGMIND2-PGMLSTD(RE)                                      
*                                                                               
INIT1B   SR    RE,RE                                                            
         IC    RE,CTRY                                                          
         CLI   CTRY,15                                                          
         BNH   *+6                                                              
         SR    RE,RE                                                            
         SLL   RE,4                                                             
         LA    RE,CTRYXLAT(RE)     INDEX INTO COUNTRY TRANSLATE TABLES          
         LM    R6,R8,4(RE)                                                      
         STM   R6,R8,VALID                                                      
*                                                                               
INIT2    TM    TSTAT6,TST6SCRP     TEST IF PROCESSING A SCRIPT                  
         BZ    INIT3                                                            
         ICM   RE,15,TBUFF         SCRUNCH HAS SET AID AND CURSOR               
         BNZ   *+6                                                              
         DC    H'0'                                                             
         MVC   TIOBAID,TBHAID-TBHDATA(RE)                                       
         MVC   TIOBCURS,TBHCURS-TBHDATA(RE)                                     
         B     TIA                                                              
*                                                                               
INIT3    ICM   RE,15,TBUFF         POINT TO INPUT BUFFER                        
         BZ    TIA                                                              
         CLI   4(RE),24            VALID AID BYTE IN BUFFER                     
         BH    *+10                                                             
         MVC   TIOBAID,4(RE)       SET PROGRAM FUNCTION KEY                     
         SR    R6,R6                                                            
         SR    R7,R7                                                            
         IC    R6,5(RE)                                                         
         IC    R7,6(RE)                                                         
         SLL   R7,26                                                            
         SRDL  R6,26                                                            
         STCM  R7,3,TIOBCURS                                                    
         NI    TIOBCURS,X'0F'      SET CURSOR SCREEN ADDRESS                    
         EJECT                                                                  
* TRANSLATE 3270 INPUT TO STANDARD INTERNAL                                     
*                                                                               
TIA      L     R2,FMRGTIAA         R2=A(TIA)                                    
         LH    R8,4(R2)                                                         
         SPACE 2                                                                
TIA0     SH    R8,=H'1'            R8=L'INPUT DATA STRING                       
         BNZ   TIA1                                                             
         CLI   SVCREQ,0            TEST ZERO INPUT TO APPLIC PROG               
         BNE   NOINPUT                                                          
         CLI   SYS,0                                                            
         BE    NOINPUT                                                          
         TM    SAVIND2,PGMINOD     TEST APPLIC PROG ALLOWS NO DATA              
         BZ    NOINPUT                                                          
         MVI   64(R2),0            YES SET END OF INPUT DATA IN TIA             
         B     TIA3                CALL MERGE ROUTINE TO SET TWA INDS           
         SPACE 2                                                                
TIA1     TM    TSTAT6,TST6SCRP     SCRUNCH HAS SET TIA TO THIS FORMAT           
         BO    TIA3                                                             
         CH    R8,=H'3'                                                         
         BL    ERROR                                                            
         BCTR  R8,0                                                             
         DROP  RF                                                               
         SPACE 2                                                                
         BAS   RE,MOVESTR          MOVE STEREO DATA BUFFER TO WORK AREA         
         BNZ   ERRORMS             MOVESTR ERROR EXIT                           
         BAS   RE,TRANSTR          TRANSLATE STERO DATA BUFFER FIELDS           
         BNZ   ERRORTS             TRANSTR ERROR EXIT                           
         SPACE 2                                                                
TIA3     GOTO1 FLDMRG,FMRGTIAA     NOW MERGE TO TWA                             
         BNZ   ERROR               EXIT WITH ERROR VALUE                        
         L     R1,FMRGSR1          RETURN TWA MERGE DATA                        
         MVC   0(TIOBL,R1),TIOBD                                                
         B     EXIT                                                             
         SPACE 2                                                                
NOINPUT  EQU   *                                                                
ERROR    XC    TIOBCNT,TIOBCNT     ZERO INPUT OR INVALID                        
         L     R1,FMRGSR1                                                       
         MVC   0(TIOBL,R1),TIOBD   EXIT WITH ZERO INPUT FIELDS COUNT            
         B     EXIT                                                             
         SPACE 2                                                                
ERRORMS  SR    R0,R0                                                            
         ICM   R0,3,MOVESERR                                                    
         DC    H'0'                                                             
ERRORTS  SR    R0,R0                                                            
         ICM   R0,3,TRANSERR                                                    
         DC    H'0'                                                             
EXIT     XMOD1 1                                                                
         EJECT                                                                  
* MOVE STEREO DATA BUFFER TO SAVE AREA - STRIPPING 3270 CONTROL                 
* CHARACTERS FROM 24 UNPROTECTED LINES.                                         
*                                                                               
* R2=A(TIA), R8=(LENGTH 3270 DATA IN TIA)                                       
*                                                                               
MOVESTR  NTR1                                                                   
         XC    MOVESERR,MOVESERR                                                
         LA    R3,STDBUF           R3=A(STEREO DATA BUFFER SAVE AREA)           
*                                  CLEAR STDBUF                                 
         LR    RE,R3                                                            
         ICM   RF,15,=AL4(L'STDBUF)                                             
         LA    R0,*                                                             
         L     R1,=F'0'                                                         
         MVCL  RE,R0                                                            
*                                  BUILD STEREO MESSAGE HEADER                  
         LA    R4,64(R2)           R4=A(FIRST SBA IN TIA)                       
         LA    R9,65(R2)                                                        
         SPACE 2                                                                
MSTR010  CLI   0(R9),X'11'         THIS CHR (SBA)                               
         BE    MSTR030                                                          
         CLI   0(R9),X'03'         THIS CHR (ETX)                               
         BE    MSTR030                                                          
MSTR020  LA    R9,1(R9)                                                         
         BCT   R8,MSTR010                                                       
         SPACE 2                                                                
MSTR030  LR    R5,R9                                                            
         SR    R5,R4               R5=L'3270 FLD                                
         SPACE 2                                                                
         SH    R5,=H'3'                                                         
         LTR   R5,R5                                                            
         BZ    MSTR050                                                          
         SPACE 2                                                                
MSTR040  MVC   0(1,R3),3(R4)                                                    
         LA    R3,1(R3)                                                         
         LA    R4,1(R4)                                                         
         BCT   R5,MSTR040                                                       
         SPACE 2                                                                
MSTR050  LR    R4,R9                                                            
         CLI   0(R4),X'03'         THIS CHR (ETX)                               
         BE    MSTREXIT                                                         
         LTR   R8,R8                                                            
         BNZ   MSTR020                                                          
         SPACE 2                                                                
MSTREXIT MVI   0(R3),0                                                          
         OC    MOVESERR,MOVESERR                                                
         BZ    *+6                                                              
         DC    H'0'                                                             
         XIT1                                                                   
         EJECT                                                                  
* TRANSLATE STEREO DATA BUFFER FIELDS TO STANDARD INTERNAL FORMAT               
*                                                                               
* R2=A(TIA)                                                                     
*                                                                               
TRANSTR  NTR1                                                                   
         XC    TRANSERR,TRANSERR                                                
         LA    R3,STDBUF           R3=A(STEREO DATA BUFFER SAVE AREA)           
         LA    R4,64(R2)           R4=A(OUTPUT IN TIA FOR TRANSLATE)            
         MVI   ROWNUM,0                                                         
         SPACE 2                                                                
         CLI   0(R3),0                                                          
         BE    TSTREND                                                          
         MVC   SFLDID,0(R3)                                                     
         LA    R3,3(R3)            BUMP PAST STEREO MESSAGE TYPE FIELD          
         TR    SFLDID,DECTRAN      PROCESS STEREO MESSAGE TYPE FIELD            
         CLI   SFLDID,SDBMHSTM     STANDARD STEREO DATA BUFFER                  
         BE    TSTMSG                                                           
         CLI   SFLDID,SDBMHMAM                                                  
         BE    TMADMSG                                                          
         CLI   SFLDID,SDBMHSOM                                                  
         BE    TSTOMSG                                                          
         B     TSTRER10                                                         
         EJECT                                                                  
TSTMSG   EQU   *                   PROCESS STEREO DATA FIELDS                   
         CLI   0(R3),0                                                          
         BE    TSTREND                                                          
         MVC   SFLDID,0(R3)                                                     
         TR    SFLDID,DECTRAN                                                   
         CLI   SFLDID,SDSRMIN                                                   
         BL    TSTM020                                                          
         CLI   SFLDID,SDSRMAX                                                   
         BH    TSTM020                                                          
         SPACE 2                                                                
         SR    RF,RF                                                            
         IC    RF,SFLDID           PROCESS STEREO ROW FIELD                     
         LA    RF,1(RF)                                                         
         SH    RF,=Y(SDSRMIN)                                                   
         STC   RF,ROWNUM                                                        
         LA    R3,1(R3)                                                         
         B     TSTMNEXT                                                         
         SPACE 2                                                                
TSTM020  MVI   PROTFLD,C'N'                                                     
         CLI   SFLDID,SDBUFMIN     TEST UNPROTECTED FIELD                       
         BL    *+12                                                             
         CLI   SFLDID,SDBUFMAX                                                  
         BNH   TSTMUNP                                                          
*                                                                               
         CLI   SFLDID,SDBPFMIN     TEST PROTECTED FIELD                         
         BL    TSTM022                                                          
         CLI   SFLDID,SDBPFMAX                                                  
         BH    TSTM022                                                          
         MVI   PROTFLD,C'Y'                                                     
         B     TSTMUNP                                                          
*                                                                               
TSTM022  CLI   SFLDID,SDBCEXP                                                   
         BE    TSTMCEX                                                          
         B     TSTRER20                                                         
         EJECT                                                                  
TSTMUNP  MVC   SFLDCOL,1(R3)       STEREO UNPROTECTED FIELD DATA                
         MVC   SFLDDLEN,2(R3)                                                   
         TR    SFLDCOL,DECTRAN                                                  
         TR    SFLDDLEN,DECTRAN                                                 
         SR    RF,RF                                                            
         IC    RF,SFLDCOL                                                       
         STC   RF,COLNUM                                                        
         BCTR  RF,0                                                             
         SR    RE,RE                                                            
         IC    RE,ROWNUM                                                        
         BCTR  RE,0                                                             
         MH    RE,=H'80'                                                        
         AR    RE,RF                                                            
         STCM  RE,3,1(R4)                                                       
         SPACE 2                                                                
TSTU010  XC    FDATA,FDATA                                                      
         SR    RF,RF                                                            
         ICM   RF,1,SFLDDLEN                                                    
         BNZ   TSTU020                                                          
         LA    RF,3(RF)                                                         
         STC   RF,0(R4)                                                         
         LA    R3,4(R3)                                                         
         CLI   PROTFLD,C'Y'                                                     
         BNE   *+6                                                              
         BCTR  R3,0                WATCH OUT FOR 0 LEN PROT FIELDS !            
         LA    R4,3(R4)                                                         
         B     TSTMNEXT                                                         
         SPACE 2                                                                
TSTU020  LA    RE,4(R3)                                                         
         CLI   PROTFLD,C'Y'                                                     
         BNE   *+6                                                              
         BCTR  RE,0                ADJUST FOR MISSING FLD IF PROT               
*                                                                               
         BCTR  RF,0                                                             
         EX    RF,*+8                                                           
         B     *+10                                                             
         MVC   FDATA(0),0(RE)                                                   
*                                                                               
         SR    RF,RF                                                            
         IC    RF,SFLDDLEN                                                      
         GOTO1 DECOMP,DMCB,FDATA,(RF),DECOMBUF                                  
         BNZ   TSTRER30                                                         
         SPACE 2                                                                
         ICM   RF,15,4(R1)                                                      
         BCTR  RF,0                                                             
         EX    RF,*+8                                                           
         B     *+10                                                             
         MVC   3(0,R4),DECOMBUF                                                 
*                                                                               
         LA    RF,4(RF)                                                         
         STC   RF,0(R4)                                                         
         LA    R4,0(RF,R4)                                                      
         IC    RF,SFLDDLEN                                                      
         LA    R3,4(RF,R3)                                                      
         CLI   PROTFLD,C'Y'                                                     
         BNE   *+6                                                              
         BCTR  R3,0                ADJUST FOR MISSING FLD IF PROT               
         B     TSTMNEXT                                                         
         SPACE 2                                                                
TSTMCEX  EQU   *                   PROCESS CURSOR EXCEPTION FIELD               
         MVC   SFLDCEXP,1(R3)                                                   
         TR    SFLDCEXP,DECTRAN                                                 
         SR    RE,RE                                                            
         SR    RF,RF                                                            
         ICM   RE,3,TIOBCURS                                                    
         IC    RF,SFLDCEXP                                                      
         AR    RE,RF                                                            
         STCM  RE,3,TIOBCURS       UPDATE CURRENT CURSOR POSITION               
         LA    R3,2(R3)                                                         
         B     TSTMNEXT                                                         
         SPACE 2                                                                
TSTMNEXT EQU   *                                                                
         B     TSTMSG                                                           
         EJECT                                                                  
TMADMSG  EQU   *                   PROCESS STEREO MAD MESSAGE                   
         LA    R0,60                                                            
TMAD010  CLI   0(R3),X'00'                                                      
         BNE   *+6                                                              
         DC    H'0'                                                             
         LA    R3,1(R3)                                                         
         BCT   R0,TMAD010                                                       
         SPACE 2                                                                
         MVC   0(3,R4),=XL3'14003E'                                             
         LA    R0,17                                                            
         LA    R4,3(R4)                                                         
TMAD020  CLI   0(R3),X'00'                                                      
         BNE   *+8                                                              
         MVI   0(R3),C' '                                                       
         MVC   0(1,R4),0(R3)                                                    
         LA    R3,1(R3)                                                         
         LA    R4,1(R4)                                                         
         BCT   R0,TMAD020                                                       
         SPACE 2                                                                
         MVC   0(3,R4),=XL3'3F0051'                                             
         LA    R0,60                                                            
         LA    R4,3(R4)                                                         
TMAD030  CLI   0(R3),X'00'                                                      
         BNE   *+8                                                              
         MVI   0(R3),C' '                                                       
         MVC   0(1,R4),0(R3)                                                    
         LA    R3,1(R3)                                                         
         LA    R4,1(R4)                                                         
         BCT   R0,TMAD030                                                       
         SPACE 2                                                                
         MVC   0(3,R4),=XL3'15008E'                                             
         LA    R0,18                                                            
         LA    R4,3(R4)                                                         
TMAD040  CLI   0(R3),X'00'                                                      
         BNE   *+8                                                              
         MVI   0(R3),C' '                                                       
         MVC   0(1,R4),0(R3)                                                    
         LA    R3,1(R3)                                                         
         LA    R4,1(R4)                                                         
         BCT   R0,TMAD040                                                       
         SPACE 2                                                                
TMAD100  LA    RF,22                                                            
         MVC   0(3,R4),=XL3'5200A1'                                             
         SPACE 2                                                                
TMAD110  LR    RE,R4                                                            
         LA    R0,79                                                            
         LA    R4,3(R4)                                                         
TMAD120  CLI   0(R3),X'00'                                                      
         BNE   *+8                                                              
         MVI   0(R3),C' '                                                       
         MVC   0(1,R4),0(R3)                                                    
         LA    R3,1(R3)                                                         
         LA    R4,1(R4)                                                         
         BCT   R0,TMAD120                                                       
         SPACE 2                                                                
         SR    R1,R1                                                            
         ICM   R1,3,1(RE)                                                       
         AH    R1,=H'80'                                                        
         STCM  R1,3,1(R4)                                                       
         MVI   0(R4),X'52'                                                      
         BCT   RF,TMAD110                                                       
         B     TSTREND                                                          
         EJECT                                                                  
TSTOMSG  EQU   *                   PROCESS STEREO OPEN MESSAGE                  
         LA    RF,24                                                            
         MVC   0(3,R4),=XL3'5200A1'                                             
         SPACE 2                                                                
TSTO010  LR    RE,R4                                                            
         LA    R0,79                                                            
         LA    R4,3(R4)                                                         
TSTO020  CLI   0(R3),X'00'                                                      
         BNE   *+8                                                              
         MVI   0(R3),C' '                                                       
         MVC   0(1,R4),0(R3)                                                    
         LA    R3,1(R3)                                                         
         LA    R4,1(R4)                                                         
         BCT   R0,TSTO020                                                       
         SPACE 2                                                                
         SR    R1,R1                                                            
         ICM   R1,3,1(RE)                                                       
         AH    R1,=H'80'                                                        
         STCM  R1,3,1(R4)                                                       
         MVI   0(R4),X'52'                                                      
         BCT   RF,TSTO010                                                       
         B     TSTREND                                                          
         EJECT                                                                  
TSTREND  EQU   *                                                                
         MVI   0(R4),0             MARK END OF DATA IN TIA                      
         B     TSTREXIT                                                         
         SPACE 2                                                                
TSTRER10 MVC   TRANSERR,=XL2'0010'                                              
         B     TSTREXIT                                                         
TSTRER20 MVC   TRANSERR,=XL2'0020'                                              
         B     TSTREXIT                                                         
TSTRER30 MVC   TRANSERR,DECOMERR                                                
         B     TSTREXIT                                                         
TSTREXIT OC    TRANSERR,TRANSERR                                                
         BZ    *+6                                                              
         DC    H'0'                                                             
         XIT1                                                                   
         EJECT                                                                  
* MERGES INPUT FIELDS IN TIA WITH FIELDS IN TWA                                 
*                                                                               
FLDMRG   NTR1                                                                   
         L     R4,FMRGTIAA                                                      
         L     R5,FMRGTWAA                                                      
         LA    R4,64(R4)           R4=A(TIA FIELD)                              
         USING IFLDHDRD,R4                                                      
         LA    R5,64(R5)           R5=A(TWA FIELD)                              
         USING FLDHDRD,R5                                                       
         SR    R2,R2                                                            
         IC    R2,IFLDLEN          R2=LENGTH OF TIA FIELD                       
         SR    R3,R3                                                            
         IC    R3,FLDLEN           R3=LENGTH OF TWA FIELD                       
         XC    FMRGERR,FMRGERR                                                  
         SPACE 2                                                                
FLDM10   MVI   FMRGFLAG,0          RESET FLAG FOR NEXT TWA FIELD                
         LTR   R2,R2                                                            
         BZ    FLDM40              TIA END                                      
         CH    R3,=H'9'                                                         
         BNL   FLDM11                                                           
         LTR   R3,R3               TEST VALID TWA FIELD                         
         BZ    *+6                                                              
         DC    H'0'                                                             
         CLI   IFLDADR,X'FF'       TEST IF ERASING INPUT FIELDS                 
         BE    FLDM40                                                           
         B     FLDMERR1            ERROR IF END OF TWA BEFORE TIA END           
*                                                                               
FLDM11   CLI   FLDATB,X'FF'        TEST NOP FIELD                               
         BE    FLDM32              DON'T DO MUCH TO IT                          
         TM    FLDATB,FATBPROT                                                  
         BO    FLDM30              BYPASS PROT FIELDS                           
         CLI   IFLDADR,X'FF'       TEST IF ERASING INPUT FIELDS                 
         BE    FLDM12                                                           
         OC    SVCRSAVE,SVCRSAVE   TEST IF SERVICE REQUEST                      
         BNZ   FLDM11A             YES                                          
* NOT A SERVICE REQUEST                                                         
         CLC   IFLDADR,FLDADR                                                   
         BE    FLDM12              PROCESS MATCHING FIELDS                      
         BH    FLDM20              CONTINUE TO NEXT                             
         B     FLDMERR2            ERROR - FIELDS DONT MATCH                    
* SERVICE REQUEST                                                               
FLDM11A  CLC   FLDADR,=XL2'003E'   TEST SERVICE REQUEST FIELD                   
         BE    FLDM12              YES / (01,63) ALWAYS PROCESSED               
         CLI   SVCRSAVE,X'02'      TEST IF CURSOR TRUNCATE                      
         BE    *+14                                                             
         CLC   IFLDADR,TIOBCURS    EXIT IF A(FIELD) >= A(CURSOR)                
         BNL   FLDM40                                                           
         LR    R7,R2                                                            
         SH    R7,=H'4'            R7=LEN OF DATA -1                            
         BM    FLDM12                                                           
         EX    R7,INPSKIP          TEST IF FLD CONTAINS >>>>>>>>...             
         BNE   FLDM12                                                           
         AR    R4,R2               YES SKIP THIS INPUT FIELD                    
         IC    R2,IFLDLEN                                                       
         B     FLDM10                                                           
*                                                                               
*        NORMAL PROCESSING - FIELD IS INPUT (BUFFER ADDRS MATCH)                
*                                                                               
FLDM12   LR    R6,R3                                                            
         SH    R6,=H'8'            R6=MAXIMUM LEN OF TWA FLD                    
         TM    FLDATB,FATBXHDR     TEST EXTENDED HEADER                         
         BZ    *+8                                                              
         SH    R6,=H'8'                                                         
         LR    R7,R2                                                            
         SH    R7,=H'3'            R7=ACTUAL LEN OF DATA                        
         BZ    FLDM13                                                           
         TM    FLDATB,FATBDEL      DELETE TRAILING BLANKS                       
         BO    FLDM13              NO                                           
         LA    RE,2(R4,R7)         YES POINT TO LAST CHR                        
         CLI   0(RE),C' '                                                       
         BNE   *+10                                                             
         BCTR  RE,0                                                             
         BCT   R7,*-10                                                          
FLDM13   CR    R6,R7                                                            
         BNL   *+6                                                              
         LR    R7,R6               TRUNC INPUT FLD DATA                         
         OC    IFLDADR,IFLDADR                                                  
         BZ    FLDM14                                                           
         CLC   FLDEOF,IFLDDATA     TEST IF END OF INPUT CHRS                    
         BNE   FLDM14                                                           
         MVC   IFLDLEN(3),=X'03FFFF'                                            
         LA    R2,3                                                             
         SR    R7,R7               SET ERASING INPUT FIELDS                     
         SPACE 2                                                                
