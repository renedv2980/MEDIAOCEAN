         ST    RE,SAVERE1                                                       
         ST    RF,SAVERF                                                        
         ST    R1,SAVER1                                                        
         MVI   MYCTAB+3,0                                                       
         CURED (P8,DUB),(14,1(R4)),MYCTAB,COMMAS=YES,                  +        
               FLOAT=-,MODADDR=ACUREDIT                                         
         L     R1,SAVER1                                                        
         L     RF,SAVERF                                                        
         L     RE,SAVERE1                                                       
         B     WFPBEDT6                                                         
*                                                                               
WFPBEDT4 DS    0H                                                               
         ST    RE,SAVERE1                                                       
         ST    RF,SAVERF                                                        
         ST    R1,SAVER1                                                        
         MVI   MYCTAB+3,2                                                       
         CURED (P8,DUB),(14,1(R4)),MYCTAB,COMMAS=YES,                  +        
               FLOAT=-,MODADDR=ACUREDIT                                         
         L     R1,SAVER1                                                        
         L     RF,SAVERF                                                        
         L     RE,SAVERE1                                                       
WFPBEDT6 TM    BYTE,X'01'          SEE IF TOTAL                                 
         BZR   RE                                                               
         MVI   15(R4),C'*'                                                      
         BR    RE                  RETURNS FOR NEXT ESCDETS ENTRY               
