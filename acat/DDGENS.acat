         GOTO1 =V(PRINTER)                                                      
         B     *+8                                                              
ERRPRNT  ST    RE,SAVERE                                                        
         LA    RE,ERRTAB                                                        
         CLI   ERROR,0             ERROR = 0 IS UNDEFINED                       
         BNE   *+6                                                              
         DC    H'0'                                                             
