         LA    R3,10(R3)                                                        
         LA    R4,9(R4)                                                         
         BCT   R5,DUMB0010                                                      
         MVC   PSECOND+119(12),SPACES                                           
         MVI   SPACING,2                                                        
         GOTO1 =A(LOCALREP),DMCB,(RC),0                                         
