*          DATA SET ACSCRREQ   AT LEVEL 038 AS OF 10/10/19                      
***********************************************************************         
*                                                                     *         
* THIS VERSION MATCHES THE UK VERSION LEVEL 36 AS OF 12/16/11         *         
*                                                                     *         
***********************************************************************         
***********************************************************************         
*  SET DYNAMIC SCREEN                                                 *         
***********************************************************************         
         SPACE 1                                                                
         USING REPFD,R2                                                         
         USING TWAELEMD,R4                                                      
         USING TWAD,R5                                                          
         SPACE 1                                                                
SCRPARM  USING TWAPARMD,APPARM                                                  
         SPACE 1                                                                
SETSCRN  NMOD1 0,*SETSCR*                                                       
         L     RC,APALOCAL                                                      
         MVI   APBYTE,1                                                         
         XC    APPARM(24),APPARM                                                
         XC    TWAELEM,TWAELEM                                                  
         ST    R5,SCRPARM.TWAPATWA                                              
         LA    R4,TWAELEM                                                       
         ST    R4,SCRPARM.TWAPAFST                                              
         LA    R3,REQTAGH                                                       
         ST    R3,SCRPARM.TWAPAOUT                                              
         MVI   TWAELCD,1                                                        
         MVI   TWAERLN,1           FIRST PLACE TO INSERT                        
         BAS   RE,FINDSCRT         R2 POINTS TO SCREEN TABLE                    
         BNE   SETSPFK                                                          
*                                                                               
SETS0205 CLI   0(R2),X'FF'         END OF LIST                                  
         BE    SETSPFK             SHOW PFKEYS                                  
         L     R6,=A(FLDTABLE)                                                  
         A     R6,APRELO                                                        
*                                                                               
SETS0210 CLI   0(R6),X'FF'                                                      
         BE    SETSPFK                                                          
         CLC   2(1,R6),REPFFLDN    MATCH ENTRY ON FIELD NUMBER                  
         BNE   SETS0250            NO, SO TRY NEXT FIELD                        
*&&DO*&& TM    REPFIND1,REPFSTO    STEREO FIELD ONLY                            
*&&DO*&& BZ    SETS0215            NO, SO DON'T CARE                            
*&&DO*&& TM    GENIND,GENSTRO      STEREO MODE                                  
*&&DO*&& BZ    SETS0500            NOT IN STEREO MODE, SO SKIP ENTRY            
*                                                                               
SETS0215 CLI   3(R6),0             DEFAULT                                      
         BE    SETS0300                                                         
         CLC   3(1,R6),APREPJCL                                                 
         BE    SETS0300                                                         
*                                                                               
SETS0250 ZIC   R0,0(,R6)                                                        
         AR    R6,R0                                                            
         B     SETS0210                                                         
*                                                                               
SETS0300 CLI   REPFFLDN,FLDNOPT6   SPECIAL SECURITY FOR APG                     
         BNE   SETS0310                                                         
*&&US                                                                           
         CLI   APGFLAG,YES                                                      
         BNE   SETS0310                                                         
         TM    SVCMPST5,CPYAPGS    SHOW $$ FIELD (OPTION 6)                     
         BZ    SETS0400            NO                                           
         TM    CUAUTH,X'20'        ARE THEY AUTHORIZED                          
         BZ    SETS0400            NO                                           
*&&                                                                             
*                                                                               
SETS0310 CLI   REPFFLDN,FLDNBUDG                                                
         BNE   SETS0320                                                         
         CLC   BUD#,APBYTE                                                      
         BL    SETS0400                                                         
         SR    RF,RF                                                            
         IC    RF,APBYTE                                                        
         LA    RF,1(,RF)                                                        
         STC   RF,APBYTE                                                        
         CLI   APBYTE,2                                                         
         BNE   SETS0315                                                         
         LA    RE,4(,R6)                                                        
         LA    R0,5                                                             
*                                                                               
SETS0312 CLI   0(RE),C'?'          MARKER                                       
         BE    SETS0313                                                         
         LA    RE,1(,RE)                                                        
         BCT   R0,SETS0312                                                      
*                                                                               
SETS0313 LA    R1,4(,R6)                                                        
         SR    RE,R1                                                            
         STH   RE,APHALF                                                        
*                                                                               
SETS0315 LA    RE,C'1'-2(,RF)                                                   
         CLI   APBYTE,11                                                        
         BL    SETS0317                                                         
         AHI   RF,-11                                                           
         LA    RE,C'A'(,RF)                                                     
*                                                                               
SETS0317 STC   RE,APFLAG                                                        
         B     SETS0330                                                         
*                                                                               
SETS0320 CLI   REPFFLDN,FLDNOFFC   OFFICE FILED ?                               
         BNE   SETS0324            NO,   SKIP                                   
         L     RF,SCRPARM.TWAPAOUT                                              
         SR    RF,R5                                                            
         STCM  RF,3,TWAOFFLD                                                    
*        MVC   TWAOFFLD,SCRPARM.TWAPAOUT                                        
*                                                                               
SETS0324 CLI   REPFFLDN,FLDNRCAP   Recap field ?                                
         BNE   SETS0330            No, skip                                     
         CLC   RECAP#,REPFDDSP+1   Recap format exsits ?                        
         BL    SETS0400            No, skip                                     
         CLI   REPFDDSP+1,1        First recap field ?                          
         BNE   SETS0330            No                                           
         SR    RF,R5               Yes                                          
         STCM  RF,3,TWARCFLD       Save displacement                            
*                                                                               
SETS0330 MVI   TWAECOL,2           FIELD STARTS @ COL 2                         
         TM    REPFIND1,REPFNXT    USE NEXT AVAILABLE FIELD?                    
         BZ    *+8                                                              
         MVI   TWAECOL,0                                                        
         TM    REPFIND1,REPFMID    START AT MIDWAY?                             
         BZ    *+8                                                              
         MVI   TWAECOL,37                                                       
         MVI   TWAEATB,X'60'       PROT,LOWER                                   
         TM    REPFIND1,REPFREQ    REQUIRED FIELD?                              
         BZ    *+8                                                              
         MVI   TWAEATB,X'68'       PROT,LOWER,HIGHLIGHT                         
         MVI   TWAEFLD,0                                                        
         XC    TWAEDTA(L'TWAELEM-TWAELLNQ),TWAEDTA                              
*                                                                               
         SR    R1,R1                                                            
         IC    R1,0(,R6)                                                        
         AHI   R1,-4               SUBTRACT TO GET CORRECT LENGTH               
         STC   R1,TWAEFLN          FIELD LENGTH                                 
         BCTR  R1,0                                                             
         EXMVC R1,TWAEDTA,4(R6)    MOVE IN DICTIONARY ESC VALUES                
*                                                                               
         CLI   REPFFLDN,FLDNBUDG                                                
         BNE   SETS0335                                                         
         LA    RE,TWAEDTA                                                       
         AH    RE,APHALF                                                        
         MVC   0(1,RE),APFLAG                                                   
*                                                                               
SETS0335 CLI   REPFFLDN,FLDNRCAP   Recap field ?                                
         BNE   SETS0345            No, skip                                     
         LA    RE,TWAEDTA          ->    Field data                             
         ZIC   RF,REPFDDSP+1       Get recap format number                      
         BCTR  RF,0                Adjust format number                         
         MHI   RF,L'RCAPFMTS       Offset to format                             
         LA    RF,RCAPFMTS(RF)     ->   Recap formats                           
         MVC   0(L'RCPCODE,RE),0(RF)    Insert recap format code                
*                                                                               
SETS0345 LA    R1,8(,R1)                                                        
         STC   R1,TWAELLN          Length of element                            
         GOTO1 VTWABLD,APPARM                                                   
         CLI   SCRPARM.TWAPERRS,0                                               
         BE    *+6                                                              
         DC    H'0'                                                             
*                                                                               
         MVI   TWAERLN,0           Same line                                    
         MVC   SCRPARM.TWAPAOUT,SCRPARM.TWAPANXT                                
         MVI   TWAECOL,20          ALL INPUT FILEDS START @ COL 20              
         TM    REPFIND1,REPFMID    START AT MIDWAY?                             
         BZ    *+8                                                              
         MVI   TWAECOL,53                                                       
         TM    REPFIND1,REPFNXT    NEXT AVAILABLE SPOT                          
         BZ    *+8                                                              
         MVI   TWAECOL,0                                                        
*                                                                               
         MVI   TWAELCD,1           ELEMENT CODE                                 
         MVI   TWAEATB,X'00'       THE INPUT FIELD                              
         MVI   TWAEFLD,0                                                        
         XC    TWAEDTA(L'TWAELEM-TWAELLNQ),TWAEDTA                              
         MVC   TWAEFLN,1(R6)       INPUT FIELD LENGTH                           
         MVI   TWAELLN,TWAELLNQ    LENGTH OF ELEM                               
*                                                                               
         GOTO1 VTWABLD,APPARM                                                   
         CLI   SCRPARM.TWAPERRS,0                                               
         BE    *+6                                                              
         DC    H'0'                                                             
*                                                                               
         L     RF,SCRPARM.TWAPAOUT GET DISP OF LAST FIELD                       
         SR    RF,R5               SUBTRACT A(TWA)                              
         STCM  RF,3,LASTTWAF                                                    
*                                                                               
         TM    REPFIND1,REPFNME    NEED NAME FIELD?                             
         BZ    SETS0400                                                         
         MVC   SCRPARM.TWAPAOUT,SCRPARM.TWAPANXT                                
         MVI   TWAERLN,0           ON THE SAME LINE                             
         MVI   TWAECOL,37                                                       
         MVI   TWAEATB,X'68'       PROT,LOWER,HIGHLIGHT                         
         MVI   TWAEFLD,0                                                        
         XC    TWAEDTA(L'TWAELEM-TWAELLNQ),TWAEDTA                              
         MVI   TWAEFLN,40          NAME FIELD LENGTH                            
         MVI   TWAELLN,TWAELLNQ+47 LENGTH OF ELEM                               
*                                                                               
         GOTO1 VTWABLD,APPARM                                                   
         CLI   SCRPARM.TWAPERRS,0                                               
         BE    *+6                                                              
         DC    H'0'                                                             
*                                                                               
SETS0400 MVC   TWAERLN,REPFIND1     NEXT LINE/NEXT AVAIL IS OUTPUT              
         NI    TWAERLN,REPF1LN+REPF2LN                                          
         MVC   SCRPARM.TWAPAOUT,SCRPARM.TWAPANXT                                
*                                                                               
SETS0500 LA    R2,REPFLNQ(,R2)      NEXT FIELD NEEDED                           
         B     SETS0205                                                         
         EJECT ,                                                                
***********************************************************************         
*  BUILD PFKEY LINES                                                  *         
***********************************************************************         
         SPACE 1                                                                
SETSPFK  MVI   TWAELCD,1           ELEMENT CODE                                 
         MVI   TWAERLN,24          STARTS AT ROW 23                             
         OI    TWAERLN,TWAERLAB                                                 
         MVI   TWAECOL,3           COLUMN 2                                     
         MVI   TWAEATB,X'68'       PROT,LOWER, HIGHLIGHTED                      
         MVI   TWAEFLD,X'FE'       FIELD NUMBER                                 
         XC    TWAEDTA(L'TWAELEM-TWAELLNQ),TWAEDTA                              
         MVC   TWAEDTA(10),=CL10'PFKEY 1'                                       
         MVI   TWAEFLN,78          INPUT FIELD LENGTH                           
         MVI   TWAELLN,TWAELLNQ+10 LENGTH OF ELEM                               
*                                                                               
         GOTO1 VTWABLD,APPARM                                                   
         CLI   SCRPARM.TWAPERRS,0                                               
         BE    *+6                                                              
         DC    H'0'                                                             
         MVC   SCRPARM.TWAPAOUT,SCRPARM.TWAPANXT                                
*                                                                               
SETSXIT  XMOD1                                                                  
         DROP  R2,R4                                                            
         DROP  SCRPARM                                                          
         EJECT ,                                                                
FINDSCRT NTR1                                                                   
         L     RF,=A(SCRTABLE)                                                  
         A     RF,APRELO                                                        
*                                                                               
FST010   CLI   0(RF),X'FF'         EOT?                                         
         BNE   FST020                                                           
         MVC   FVMSGNO,=AL2(2090)                                               
         SR    R2,R2                                                            
         B     FST090                                                           
*                                                                               
FST020   CLC   APREPJCL,0(RF)                                                   
         BNE   FST025                                                           
         LA    RE,NO               ASSUME NO                                    
         TM    1(RF),REPIAPG                                                    
         BZ    *+8                                                              
         LA    RE,YES              YES                                          
         CLM   RE,1,APGFLAG        TEST APG YES/NO                              
         BE    FST030                                                           
*                                                                               
FST025   LA    RF,4(,RF)                                                        
         B     FST010                                                           
*                                                                               
FST030   SR    R2,R2                                                            
         ICM   R2,3,2(RF)                                                       
         A     R2,=A(REPFLDS)                                                   
         A     R2,APRELO                                                        
         SR    RE,RE                                                            
*                                                                               
FST090   LTR   RE,RE                                                            
         XIT1  REGS=(R2)                                                        
         SPACE 1                                                                
         DROP  R5                  KEEP IT CLEAN                                
         SPACE 1                                                                
         LTORG                                                                  
         EJECT ,                                                                
***********************************************************************         
*                    FIELD LENGTH EQUATES                             *         
*                           I.E.                                      *         
*  LENGTH OF FIELDS ON SCREEN INCLUDING THE EXCLUDE CHARACTER ("*")   *         
*                                                                     *         
*  NOTE: (#) MEANS DUE TO +/- LIST SUPPORT                            *         
***********************************************************************         
         SPACE 1                                                                
LENFUNLG EQU   LUNLG               UNIT        LEDGER                           
LENFACCT EQU   LACCOUNT+1          ACCOUNT                                      
LENFAC13 EQU   LACCOUNT+1          ACCOUNT     LENGTH   13                      
LENFAC15 EQU   LULACNT+1           ACCOUNT     LENGTH   15                      
LENFCNTR EQU   LULACNT+1           CONTRA      ACCOUNT                          
LENFBGRP EQU   4                   BILLING     GROUP                            
LENFBSCR EQU   LACCOUNT+1          BILLING     SOURCE                           
LENFBILT EQU   2                   BILLING     TYPE                             
LENFTYPE EQU   1                   TYPE                                         
LENFCLNT EQU   LACCOUNT+1          CLIENT                                       
LENFOFFC EQU   3                   OFFICE                                       
LENFOGRP EQU   2                   OFFICE      GROUP                            
LENFDATE EQU   10                  DATE                                         
LENFMOAR EQU   22                  MONTH OF    ACTIVITY RANGE                   
LENFWKCD EQU   6                   WORK        CODE     (#)                     
LENFWCGP EQU   2                   WORK        CODE     GROUP                   
LENFFLTR EQU   2                   FILTER                                       
LENFMEDA EQU   2                   MEDIA                                        
LENFMGRP EQU   2                   MEDIA       GROUP                            
LENFPRDR EQU   26                  PERIOD      RANGE                            
LENFLOCS EQU   9                   LOCATION    STATUS                           
LENFRCON EQU   1                   RECONCILED  ITEMS                            
LENFTTYP EQU   4                   TRANSACTION TYPE                             
LENFESTS EQU   1                   ESTIMATE    STATUS                           
LENFSTTY EQU   5                   STUDIO      TYPE                             
LENFUSRF EQU   3                   USER        FIELD                            
LENFPRSC EQU   8                   PERSON                                       
LENFSELT EQU   6                   SELECT                                       
LENFMTHD EQU   3                   METHOD                                       
LENFNARA EQU   6                   NARRATIVE                                    
LENFOPTN EQU   1                   OPTION                                       
LENFCURC EQU   4                   CURRENCY    CONVERSION                       
LENFOVHD EQU   6                   OVERHEAD                                     
LENFRCAP EQU   1                   RECAP                                        
LENFFRPT EQU   1                   FORMAT REPORT                                
LENFAUTH EQU   20                  Authorization                                
LENFBUDG EQU   LINPBUDG            BUDGET                                       
         EJECT ,                                                                
***********************************************************************         
*  Table of screen to load based on type                              *         
***********************************************************************         
         SPACE 1                                                                
SCRTABLE DC    AL1(REPJCLV,0),AL2(REPFLDSV-REPFLDS)                             
         DC    AL1(REPJCLI,0),AL2(REPFLDSI-REPFLDS)                             
         DC    AL1(REPJCL1,0),AL2(REPFLDSL-REPFLDS)                             
         DC    AL1(REPJCLP,0),AL2(REPFLDSP-REPFLDS)                             
         DC    AL1(REPJCLR,0),AL2(REPFLDSR-REPFLDS)                             
         DC    AL1(REPJCLX,0),AL2(REPFLDSX-REPFLDS)                             
         DC    AL1(REPJCL2,0),AL2(REPFLD1C-REPFLDS)                             
         DC    AL1(REPJCLB,0),AL2(REPFLDSC-REPFLDS)                             
         DC    AL1(REPJCLG,0),AL2(REPFLDGL-REPFLDS)                             
         DC    AL1(REPJCLZ,0),AL2(REPFLDSZ-REPFLDS)                             
         DC    AL1(REPJCLFI,REPIAPG),AL2(REPFLDFI-REPFLDS)                      
         DC    AL1(REPJCLM2,REPIAPG),AL2(REPFLDM2-REPFLDS)                      
*        DC    AL1(REPJCLIV,REPIAPG),AL2(REPFLDIV-REPFLDS)                      
         DC    X'FF'                                                            
         SPACE 1                                                                
REPIAPG  EQU   X'80'                                                            
         EJECT ,                                                                
***********************************************************************         
*  XL1 - LENGTH OF ENTRY                                              *         
*  XL1 - LENGTH OF FIELD - INCLUDING THE EXCLUDE CHARACTER            *         
*  XL1 - FIELD NUMBER                                                 *         
*  0C  - FIELD TITLE                                                  *         
***********************************************************************         
         SPACE 1                                                                
