         DC    AL1(FTBUNLGX-*,LENFUNLG,FLDNUNLG,0)                              
         DCDD  AC#UNTLD,14                                                      
FTBUNLGX EQU   *                                                                
*                                                                               
         DC    AL1(FTBACCTX-*,LENFACCT,FLDNACCT,0)                              
         DCDD  AC#ACC,14                                                        
FTBACCTX EQU   *                                                                
*                                                                               
         DC    AL1(FTBCNT1X-*,LENFCNTR,FLDNCNTR,REPJCL1)                        
*&&UK                                                                           
         DCDD  AC#CSTNC,14                                                      
*&&                                                                             
*&&US*&& DC    CL14'Cost/Non-Cli'                                               
FTBCNT1X EQU   *                                                                
*                                                                               
         DC    AL1(FTBCNT2X-*,LENFCNTR,FLDNCNTR,0)                              
         DCDD  AC#CTRA,14                                                       
FTBCNT2X EQU   *                                                                
*                                                                               
         DC    AL1(FTBBGRPX-*,LENFBGRP,FLDNBGRP,0)                              
         DCDD  AC#BLGGP,14                                                      
FTBBGRPX EQU   *                                                                
*                                                                               
         DC    AL1(FTBOGRPX-*,LENFOGRP,FLDNOGRP,0)                              
         DCDD  AC#OFFGP,14                                                      
FTBOGRPX EQU   *                                                                
*                                                                               
         DC    AL1(FTBOFCCX-*,LENFOFFC,FLDNOFCC,0)                              
*&&UK                                                                           
         DCDD  AC#RSOCC,14                                                      
*&&                                                                             
*&&US*&& DC    C'Client office'                                                 
FTBOFCCX EQU   *                                                                
*                                                                               
         DC    AL1(FTBOFFCX-*,LENFOFFC,FLDNOFFC,0)                              
         DCDD  AC#OFF,17                                                        
FTBOFFCX EQU   *                                                                
*                                                                               
         DC    AL1(FTBMGRPX-*,LENFMGRP,FLDNMGRP,0)                              
         DCDD  AC#MEDGP,14                                                      
FTBMGRPX EQU   *                                                                
*                                                                               
         DC    AL1(FTBMEDAX-*,LENFMEDA,FLDNMEDA,0)                              
         DCDD  AC#MED,14                                                        
FTBMEDAX EQU   *                                                                
*                                                                               
         DC    AL1(FTBWCGPX-*,LENFWCGP,FLDNWCGP,0)                              
         DCDD  AC#WCGRP,14                                                      
FTBWCGPX EQU   *                                                                
*                                                                               
         DC    AL1(FTBTASKX-*,LENFWKCD,FLDNTASK,REPJCL1)                        
         DCDD  AC#TASK,14                                                       
FTBTASKX EQU   *                                                                
*                                                                               
         DC    AL1(FTBWKCDX-*,LENFWKCD,FLDNWKCD,0)                              
         DCDD  AC#WC,14                                                         
FTBWKCDX EQU   *                                                                
*                                                                               
         DC    AL1(FTBSTDTX-*,LENFDATE,FLDNSTDT,0)                              
         DCDD  AC#STRDT,14                                                      
FTBSTDTX EQU   *                                                                
*                                                                               
         DC    AL1(FTBENDTX-*,LENFDATE,FLDNENDT,0)                              
         DCDD  AC#ENDDT,14                                                      
FTBENDTX EQU   *                                                                
*                                                                               
         DC    AL1(FTBMOARX-*,LENFMOAR,FLDNMOAR,0)                              
         DCDD  AC#MOARA,14                                                      
FTBMOARX EQU   *                                                                
*&&US                                                                           
         DC    AL1(FTBMMOSX-*,LENFMOAR,FLDNMMOS,0)                              
         DCDD  AC#MMOSR,15                                                      
FTBMMOSX EQU   *                                                                
*&&                                                                             
         DC    AL1(FTBACTSX-*,LENFDATE,FLDNACTS,0)                              
         DCDD  AC#ACTST,14                                                      
FTBACTSX EQU   *                                                                
*                                                                               
         DC    AL1(FTBACTEX-*,LENFDATE,FLDNACTE,0)                              
         DCDD  AC#ACTND,14                                                      
FTBACTEX EQU   *                                                                
*                                                                               
         DC    AL1(FTBALTSX-*,LENFDATE,FLDNALTS,0)                              
         DCDD  AC#ALTSD,15                                                      
FTBALTSX EQU   *                                                                
*                                                                               
         DC    AL1(FTBALTEX-*,LENFDATE,FLDNALTE,0)                              
         DCDD  AC#ALTED,14                                                      
FTBALTEX EQU   *                                                                
*                                                                               
         DC    AL1(FTBOCURX-*,LENFDATE,FLDNOCUR,0)                              
*&&UK                                                                           
         DCDD  AC#OVRCU,16                                                      
*&&                                                                             
*&&US*&& DC    C'Override current'                                              
FTBOCURX EQU   *                                                                
*                                                                               
         DC    AL1(FTBPRDRX-*,LENFPRDR,FLDNPRDR,0)                              
*&&UK                                                                           
         DCDD  AC#RLPRA,16                                                      
*&&                                                                             
*&&US*&& DC    C'Period Range'                                                  
FTBPRDRX EQU   *                                                                
