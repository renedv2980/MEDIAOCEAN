*---------------------------------------------------------------------*         
*                                                                               
*---------------------------------------------------------------------*         
         MACRO                                                                  
         TEXT  &TEXT                                                            
         LCLC  &STRING                                                          
&DINK    SETC  ''''                                                             
&STRING  SETC  '&TEXT'                                                          
         AIF   (T'&STRING EQ 'O').XIT                                           
&LEN     SETA  K'&STRING                                                        
&LPOS    SETA  ('&STRING' INDEX '&DINK')                                        
         AIF   (&LPOS NE 1).A                                                   
&LEN     SETA  &LEN-2                                                           
&STRING  SETC  '&STRING'(2,&LEN)                                                
.A       DC    CL&LEN'&STRING'                                                  
.XIT     MEXIT                                                                  
         MEND                                                                   
*---------------------------------------------------------------------*         
* Formating of a string text                                                    
*---------------------------------------------------------------------*         
         MACRO                                                                  
         FMT &CODE=,&NAME=                                                      
                                                                                
         AIF   (T'&CODE EQ 'O').ER1                                             
&CODE    DC    X'2D0200',CL8'&CODE',CL31' '                                     
         DC    XL2'00',XL5'00'                                                  
         AIF   (T'&NAME EQ 'O').B                                               
&CODE.1  DC    AL1(NAMELQ,&CODE.2-&CODE.1)                                      
         TEXT  &NAME                                                            
&CODE.2  EQU   *                                                                
.B       ANOP                                                                   
         AGO    .XIT                                                            
ER1      MNOTE  1,'MISSING CODE OF FORMAT'                                      
.XIT     MEXIT                                                                  
         MEND                                                                   
         EJECT                                                                  
*---------------------------------------------------------------------*         
* Report type                                                                   
*---------------------------------------------------------------------*         
         MACRO                                                                  
*&&UK*&& REPTY &TYPE,&LB                                                        
*&&US*&& REPTY &TYPE                                                            
         LCLC  &RTYPE                                                           
         AIF   (T'&TYPE EQ 'O').ER1                                             
         AIF   ('&TYPE' NE 'AC#GLG').TYP10                                      
*        AIF   ('&TYPE' NE 'AC#RS540').TYP10                                    
&RTYPE   SETC  '199'                                                            
.TYP10   ANOP                                                                   
*&&UK                                                                           
&LB      DC    AL1(STYELQ,STYLNQ,&RTYPE)                                        
*&&                                                                             
*&&US                                                                           
RTYPE    DC    AL1(STYELQ,STYLNQ,&RTYPE)                                        
*&&                                                                             
         DC    AL1(ESC#LFJT),AL2(&TYPE),AL1(6),CL2' '                           
         DC    AL1(0,STYSACNT,0)                                                
         AGO    .XIT                                                            
ER1      MNOTE  1,'Missing reporting type'                                      
.XIT     MEXIT                                                                  
         MEND                                                                   
         EJECT                                                                  
*---------------------------------------------------------------------*         
* Report type 2                                                                 
*---------------------------------------------------------------------*         
         MACRO                                                                  
*&&UK*&& REPT2 &TYPE,&LB2                                                       
*&&US*&& REPT2 &TYPE                                                            
         LCLC  &RTYP2                                                           
         AIF   (T'&TYPE EQ 'O').ER2                                             
         AIF   ('&TYPE' NE 'AC#RSPAY').TY210                                    
&RTYP2   SETC  '212'                                                            
.TY210   ANOP                                                                   
*&&UK                                                                           
&LB2     DC    AL1(STYELQ,STYLNQ2,&RTYP2)                                       
*&&                                                                             
*&&US                                                                           
RTYP2    DC    AL1(STYELQ,STYLNQ2,&RTYP2)                                       
*&&                                                                             
         DC    AL1(ESC#LFJT),AL2(&TYPE),AL1(6),CL2' '                           
         DC    AL1(0,0,0,0,0,0,0)                                               
         DC    AL1(0,0,0,0,0,0,0)                                               
         DC    AL1(54)                    WIDTH OF REPORT                       
*        DC    AL1(217)                   WIDTH OF REPORT                       
         AGO    .XIT                                                            
ER2      MNOTE  1,'Missing reporting type'                                      
.XIT     MEXIT                                                                  
         MEND                                                                   
         EJECT                                                                  
*---------------------------------------------------------------------*         
* Row macro                                                                     
*---------------------------------------------------------------------*         
         MACRO                                                                  
&TAG     ROW   &KEYWORD=,                                              +        
               &TOTAL=N,                                               +        
               &PREFIX=,                                               +        
               &HEADING=L,                                             +        
               &SHOW=B,                                                +        
               &LEVEL=N,                                               +        
               &DICT=N                                                          
.*                                                                              
         LCLC &OPT1                                                             
         LCLC &OPT2                                                             
         LCLC &HDTY                                                             
         GBLA &ROW#                                                             
         AIF   (T'&KEYWORD EQ 'O').ER1                                          
&ROW#    SETA  &ROW#+1                                                          
&HDTY    SETC  'RRWLHEAD'                                                       
&DLEN    SETA  2                                                                
&OPT1    SETC  '0'                                                              
&OPT2    SETC  '0'                                                              
         AIF   ('&HEADING'(1,1) EQ 'L').HD                                      
&HDTY    SETC  'RRWRHEAD'                                                       
         AIF   ('&HEADING'(1,1) EQ 'R').HD                                      
&HDTY    SETC  'RRWCHEAD'                                                       
         AIF   ('&HEADING'(1,1) EQ 'C').HD                                      
&HDTY    SETC  'RRWMID'                                                         
         AIF   ('&HEADING'(1,1) EQ 'M').HD                                      
&HDTY    SETC  '0'                                                              
         AIF   ('&HEADING'(1,1) EQ 'B').HD                                      
         AGO   .ER3                                                             
.HD      ANOP                                                                   
         AIF   (T'&LEVEL EQ 'O').CN                                             
         AIF   ('&LEVEL'(1,1) NE 'Y').CN                                        
&OPT2    SETC  'RRWLVL'                                                         
.CN      ANOP                                                                   
&DLEN    SETA  &DLEN+2                                                          
         AIF   (T'&SHOW EQ 'O').B                                               
&OPT1    SETC  'RRWCODE+RRWNAME+RRWNEWEL'                                       
         AIF   ('&SHOW'(1,1) EQ 'B').B                                          
&OPT1    SETC  'RRWCODE+RRWNEWEL'                                               
         AIF   ('&SHOW'(1,1) EQ 'C').B                                          
&OPT1    SETC  'RRWNAME+RRWNEWEL'                                               
         AIF   ('&SHOW'(1,1) NE 'N').ER2                                        
                                                                                
&DLEN    SETA  &DLEN+2                                                          
.B       ANOP                                                                   
RWA&ROW# DC    AL1(RRWELQ,RWX&ROW#-RWA&ROW#,&ROW#,&OPT1)                        
         DC    AL1(&HDTY,&OPT2,0,0,0)                                           
         DC    AL1(&DLEN,RWX&ROW#-RWT&ROW#,0,0,0)                               
         DC    AL1(0,0,0)                                                       
         DC    AL2(&KEYWORD)                                                    
         AIF   (T'&SHOW EQ 'O').P8                                              
         AIF   ('&SHOW'(1,1) NE 'B').P1                                         
         DC    AL2(AC#RSCAN)                                                    
.P1      AIF   ('&SHOW' NE 'C').P2                                              
         DC    AL2(AC#RSCOD)                                                    
.P2      AIF   ('&SHOW' NE 'N').P8                                              
         DC    AL2(AC#RSNAM)                                                    
.P8      AIF   (T'LEVEL EQ 'O').P10                                             
         AIF   ('&LEVEL' NE 'Y').P10                                            
         DC    AL2(AC#RSLVL)                                                    
.P10     ANOP                                                                   
RWT&ROW# EQU   *                                                                
         AIF   (T'&PREFIX EQ 'O').P12                                           
         TEXT  &PREFIX                                                          
.P12     ANOP                                                                   
RWX&ROW# EQU   *                                                                
         AGO   .XIT                                                             
                                                                                
.ER1     MNOTE 1,'Missing keyword'                                              
         AGO   .XIT                                                             
.ER2     MNOTE 1,'Not valid show value'                                         
         AGO   .XIT                                                             
.ER3     MNOTE 1,'Not valid heading type'                                       
                                                                                
.XIT     MEXIT                                                                  
         MEND                                                                   
         EJECT                                                                  
