         OR    RU(3)=1CR                                                        
         OR    RU(3)=1CU                                                        
         OR    RU(3)=1CZ                                                        
         OR    RU(3)=1C1                                                        
         OR    RU(3)=1C2                                                        
         OR    RU(3)=1C3                                                        
