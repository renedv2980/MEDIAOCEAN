./ ADD NAME=DPW3CNTL                                                            
*---------------------------------------------------------------------*         
* CONTROL CARDS FOR USING(DPW3)                                       *         
*---------------------------------------------------------------------*         
*                                                                               
* GET THE TIME PERIOD DEMOS FOR EACH QH OF DAYPART HUT/PUT DATA.                
*                                                                               
* THIS IS AN INNER JOIN: THROW AWAY QUARTER HOURS THAT ARE NOT ON THE           
* TIME PERIOD FILE (EX: EXCLUSION QHS ARE NOT INCLUDED IN THE AVERAGE).         
* THROW AWAY QUARTER HOURS THAT ARE NOT PRESENT ON THE DAYPART HUT/PUT          
* FILE.                                                                         
* SEE D1F2CNTL FOR PRE-PROCESSING OF FILE F2.                                   
                                                                                
 JOINKEYS F1=HUTPUTF,                                                           
          FIELDS=(WD_KRSRVC,A,WD_KMKTC,A,WD_KRPRD,A,WD_KRPYR,A,                 
                  WD_KSPEXC,A,WD_KPBACK,A,WD_KSAMTY,A,WD_KSUBSI,A,              
                  WD_KWEEK,A,WD_KDAY,A,WD_KQHR,A)                               
 JOINKEYS F2=QHROUT,TASKID=D1,                                                  
          FIELDS=(WD_KRSRVC,A,WD_KMKTC,A,WD_KRPRD,A,WD_KRPYR,A,                 
                  WD_KSPEXC,A,WD_KPBACK,A,WD_KSAMTY,A,WD_KSUBSI,A,              
                  WD_KWEEK,A,WD_KDAY,A,WD_KQHR,A)                               
*                                                                               
* KEEP RECORDS THAT HAVE BOTH HUTS/PUTS AND TSA DEMOS. ALSO KEEP                
* HUT/PUT RECORDS WITHOUT ANY TSA DEMOS. THIS COVERS THE SCENARIO               
* WHERE THERE IS NO TSA DATA (EX: LIVE+SD)                                      
*                                                                               
 JOIN UNPAIRED,F1                                                               
*                                                                               
 REFORMAT FIELDS=(F2:WD_KAREA,F1:WD_MHQREC,F1:WD_DHRREC,F1:WD_PNRAREA,          
                  F1:WD_QDRREC,F2:WD_STRREC,F1:WD_CDRREC,F1:WD_MISREC),         
                  FILL=X'00'                                                    
                                                                                
* SORT JOINED RECORDS ON DAYPART/STATION/WEEK                                   
 SORT FIELDS=(WD_KRSRVC,A,WD_KMKTC,A,WD_KRPRD,A,WD_KRPYR,A,                     
              WD_KSPEXC,A,WD_KPBACK,A,WD_KSAMTY,A,WD_KSUBSI,A,                  
              WD_CDRDPART,A,WD_KDISC,A,WD_KWEEK,A),EQUALS                       
                                                                                
* CREATE WEEKLY AVERAGES FOR DMA AND TSA IMPRESSIONS, FOR EVERY DAYPART         
* AND STATION. RESULTS ARE TRUNCATED.                                           
                                                                                
 OUTFIL REMOVECC,NODETAIL,                                                      
       SECTIONS=(WD_KRSRVC,WD_KMKTC,WD_KRPRD,WD_KRPYR,                          
                 WD_KSPEXC,WD_KPBACK,WD_KSAMTY,WD_KSUBSI,                       
                 WD_CDRDPART,WD_KDISC,WD_KWEEK,                                 
           TRAILER3=(WD_KAREA:WD_KAREA,                                         
                     WD_MHQREC:WD_MHQREC,                                       
                     WD_DHRREC:WD_DHRREC,                                       
                     WD_PNRAREA:WD_PNRAREA,                                     
                     WD_QDRREC:WD_QDRREC,                                       
                     WD_STRHEAD:WD_STRHEAD,                                     
                     WD_STRHHLDS:AVG=(WD_STRHHLDS,TO=BI,LENGTH=4),              
                     WD_STRCH25:AVG=(WD_STRCH25,TO=BI,LENGTH=4),                
                     WD_STRCH611:AVG=(WD_STRCH611,TO=BI,LENGTH=4),              
                     WD_STRM1214:AVG=(WD_STRM1214,TO=BI,LENGTH=4),              
                     WD_STRM1517:AVG=(WD_STRM1517,TO=BI,LENGTH=4),              
                     WD_STRM1820:AVG=(WD_STRM1820,TO=BI,LENGTH=4),              
                     WD_STRM2124:AVG=(WD_STRM2124,TO=BI,LENGTH=4),              
                     WD_STRM2534:AVG=(WD_STRM2534,TO=BI,LENGTH=4),              
                     WD_STRM3549:AVG=(WD_STRM3549,TO=BI,LENGTH=4),              
                     WD_STRM5054:AVG=(WD_STRM5054,TO=BI,LENGTH=4),              
                     WD_STRM5564:AVG=(WD_STRM5564,TO=BI,LENGTH=4),              
                     WD_STRM65P:AVG=(WD_STRM65P,TO=BI,LENGTH=4),                
                     WD_STRF1214:AVG=(WD_STRF1214,TO=BI,LENGTH=4),              
                     WD_STRF1517:AVG=(WD_STRF1517,TO=BI,LENGTH=4),              
                     WD_STRF1820:AVG=(WD_STRF1820,TO=BI,LENGTH=4),              
                     WD_STRF2124:AVG=(WD_STRF2124,TO=BI,LENGTH=4),              
                     WD_STRF2534:AVG=(WD_STRF2534,TO=BI,LENGTH=4),              
                     WD_STRF3549:AVG=(WD_STRF3549,TO=BI,LENGTH=4),              
                     WD_STRF5054:AVG=(WD_STRF5054,TO=BI,LENGTH=4),              
                     WD_STRF5564:AVG=(WD_STRF5564,TO=BI,LENGTH=4),              
                     WD_STRF65P:AVG=(WD_STRF65P,TO=BI,LENGTH=4),                
                     WD_STRWW:AVG=(WD_STRWW,TO=BI,LENGTH=4),                    
                     WD_CDRREC:WD_CDRREC,                                       
                     WD_MISREC:WD_MISREC))                                      
