   JOINKEYS F2=PRSWRK2,FIELDS=(W_KSEQ,A,W_KDISC,A),SORTED                       
   REFORMAT FIELDS=(F1:W_KFILETYPE,                                             
                       W_KSEQ,                                                  
                    F2:W_KWEEK,                                                 
                       W_KDAY,                                                  
                       W_KDAY_ALF,                                              
                       W_KCODE,                                                 
                    F1:W_KRSRVC,                                                
                       W_KMKTC,                                                 
                       W_KRPRD,                                                 
