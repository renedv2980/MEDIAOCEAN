             FIELDS=(W_KRSRVC,A,                                                
                     W_KMKTC,A,                                                 
                     W_KRPRD,A,                                                 
                     W_KRPYR,A,                                                 
                     W_KSPEXC,A,                                                
                     W_KPBACK,A,                                                
                     W_KSAMTY,A,                                                
                     W_KSUBSI,A,                                                
                     W_KDAY,A,                                                  
                     W_KQHR,A,                                                  
                     W_KAWKS,A)        *** LOOK FOR "ALL-WEEK"                  
 JOINKEYS F2=TPHUTAVG,                                                          
             FIELDS=(W_KRSRVC,A,                                                
                     W_KMKTC,A,                                                 
                     W_KRPRD,A,                                                 
                     W_KRPYR,A,                                                 
                     W_KSPEXC,A,                                                
                     W_KPBACK,A,                                                
                     W_KSAMTY,A,                                                
                     W_KSUBSI,A,                                                
                     W_KDAY,A,                                                  
                     W_KQHR,A,                                                  
                     W_KAWKS,A)                                                 
