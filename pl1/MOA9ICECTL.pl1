 052:'---------------',                                                         
 069:'---------------',                                                         
 086:'---------------',                                                         
 103:'---------------',/),                                                      
 SECTIONS=(S2SYSTM,                                                             
 TRAILER3=(12:S2ALPHA,17:S2SYSTM,                                               
 035:TOTAL=(S2ORDER,                                                            
           EDIT=(SIIIIIIIIIIT.TT),SIGNS=(,-,,),LENGTH=15),                      
 052:TOTAL=(S2PAID,                                                             
           EDIT=(SIIIIIIIIIIT.TT),SIGNS=(,-,,),LENGTH=15),                      
 069:TOTAL=(S2BILLD,                                                            
           EDIT=(SIIIIIIIIIIT.TT),SIGNS=(,-,,),LENGTH=15),                      
