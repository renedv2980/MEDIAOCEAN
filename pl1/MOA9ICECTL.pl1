 OMIT COND=(A9ID,CH,EQ,C'SA9HDR',OR,                                            
            A9SPYY01,BI,EQ,X'00000000',OR,                                      
            (A9SPOR01,EQ,+00,AND,                                               
            A9SPPA01,EQ,+00,AND,                                                
            A9SPBI01,EQ,+00))                                                   
 INREC  OVERLAY=(A9SPYY01:A9SPYY01,TRAN=ALTSEQ)                                 
 ALTSEQ CODE=(FAF0,FBF1,FCF2,FDF3,FEF4,FF00)                                    
 OUTFIL FTOV,FNAMES=VBOUTFIL                                                    
