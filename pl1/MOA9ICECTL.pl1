*                                                                               
* SORT FOR MONEY FLOW OUTPUT FILE.                                              
*                                                                               
./ ADD NAME=SRT1CNTL   ** SORT 1 FOR OUTPUT FILE                                
 OPTION NOCHALT,VLSHRT                                                          
 SORT FIELDS=(S1SYSTM,A,S1ALPHA,A,S1MED,A,S1CLT,A,S1PRD,A,                      
              S1EST,A,S1TYPE,A,S1ACAS,A,S1MOFF,A,S1AOFF,A,                      
              S1YYMM,A)                                                         
 OUTFILE FNAMES=SORTFIL1                                                        
*                                                                               
* OUTPUT FILE WILL BE DYNTAMICALLY ALLOCATED                                    
* FORMAT WILL BE DDSPROD.MOFLOW.&&&&.A9##.DYYMMDD.$$$                           
* &&&& IS THE AGENCY ID ON THE ACCESS RECORD                                    
* ##   IS THE ALPHA  ID                                                         
* $$$  IS EITHER NET OR GRS BASED ON THE OPTION FROM SYSIN                      
*                                                                               
*                                                                               
* SORT FOR MONEY FLOW OUTPUT FILE.                                              
*                                                                               
./ ADD NAME=OUTFCNTL   ** OUTPUT FOR MONEY FLOW                                 
 OPTION COPY,NOCHALT,VLSHRT                                                     
 INREC  IFTHEN=(WHEN=(FILEHDR,CH,NE,C'HDR'),                                    
               OVERLAY=(S1MEDNM:S1MEDNM,TRAN=ALTSEQ,                            
               S1CLTNM:S1CLTNM,TRAN=ALTSEQ,                                     
               S1PRDNM:S1PRDNM,TRAN=ALTSEQ,                                     
               S1ESTNM:S1ESTNM,TRAN=ALTSEQ,                                     
               S1TYPE:S1TYPE,TRAN=ALTSEQ)),                                     
        IFTHEN=(WHEN=(FILEHDR,CH,EQ,C'HDR'),                                    
               OVERLAY=(FILEOPT1:FILEOPT1,TRAN=ALTSEQ,                          
                        FILEOPT2:FILEOPT2,TRAN=ALTSEQ))                         
 ALTSEQ CODE=(0040,E1A1,DF9F)                                                   
 OUTREC IFTHEN=(WHEN=(FILEHDR,CH,NE,C'HDR'),                                    
                BUILD=(OUTFRDW,                                                 
                S1SYSTM,DELIMITER,                                              
                S1ALPHA,DELIMITER,                                              
                S1MED,DELIMITER,                                                
                S1MEDNM,DELIMITER,                                              
                S1CLT,DELIMITER,                                                
                S1CLTNM,DELIMITER,                                              
                S1PRD,DELIMITER,                                                
                S1PRDNM,DELIMITER,                                              
                S1EST,DELIMITER,                                                
                S1ESTNM,DELIMITER,                                              
                S1TYPE,DELIMITER,                                               
                S1ACAS,EDIT=(T),DELIMITER,                                      
                S1MOFF,DELIMITER,                                               
                S1AOFF,DELIMITER,                                               
*                                                                               
               S1YYMM,                                                          
               Y2T,EDIT=(TTTT/TT),LENGTH=7,DELIMITER,                           
* ORDERED                                                                       
               S1ORDER,                                                         
               PD,EDIT=(SIIIIIIIIIIT.TT),SIGNS=(,-,,),LENGTH=15,                
               DELIMITER,                                                       
* PAID                                                                          
               S1PAID,                                                          
               PD,EDIT=(SIIIIIIIIIIT.TT),SIGNS=(,-,,),LENGTH=15,                
               DELIMITER,                                                       
* BILLED                                                                        
               S1BILLD,                                                         
               PD,EDIT=(SIIIIIIIIIIT.TT),SIGNS=(,-,,),LENGTH=15,                
               DELIMITER,                                                       
* ASSIGNED                                                                      
               S1ASSIN,                                                         
               PD,EDIT=(SIIIIIIIIIIT.TT),SIGNS=(,-,,),LENGTH=15,                
               DELIMITER,                                                       
* INSERTION ORDER                                                               
               S1INORD,                                                         
               PD,EDIT=(SIIIIIIIIIIT.TT),SIGNS=(,-,,),LENGTH=15))               
*                                                                               
 OUTFILE FNAMES=OUTFILE,                                                        
               REMOVECC,                                                        
               TRAILER1=('TRL',DELIMITER,                                       
               COUNT=(M11,LENGTH=10))                                           
**********************************************************************          
* COPY OUTPUT FILE TO DDSTEST.FTP FILE                                          
**********************************************************************          
./ ADD NAME=TST2CNTL                                                            
 OPTION COPY,NOCHALT,VLSHRT                                                     
 OUTFILE FNAMES=OUTTEST                                                         
*                                                                               
**********************************************************************          
* SECOND SORT IS FOR REPORTING. SHOWS TOTALS FOR                                
* ORDERED, PAID, BILLED, ASSIGNED DOLLARS AND INSERTION ORDERS                  
*                                                                               
**********************************************************************          
./ ADD NAME=SRT2CNTL   ** SECOND SORT FOR REPORTING                             
 OMIT COND=(FILEHDR,CH,EQ,C'HDR')                                               
 SORT FIELDS=(S1SYSTM,A,S1ALPHA,A,S1MED,A,S1MOFF,A,S1CLT,A,S1YYMM,A)            
 SUM FIELDS=(S1ORDER,S1PAID,S1BILLD,S1ASSIN,S1INORD)                            
 OUTREC BUILD=(S1FRDW,                                                          
               S1SYSTM,                                                         
               S1ALPHA,                                                         
               S1MED,                                                           
               S1MOFF,                                                          
               S1CLT,                                                           
               S1YYMM,                                                          
               S1ORDER,                                                         
               S1PAID,                                                          
               S1BILLD,                                                         
               S1ASSIN,                                                         
               S1INORD,                                                         
               70Z)                                                             
*********************************************************************           
* REPORTS ARE BY                                                                
*    1. SYSTEM, ALPHA                                                           
*    2. SYSTEM, ALPHA, MEDIA                                                    
*    3. SYSTEM, ALPHA, MEDIA, MEDIA OFFICE                                      
*    4. SYSTEM, ALPHA, MEDIA, MEDIA OFFICE, CLIENT                              
*                                                                               
*********************************************************************           
./ ADD NAME=SUM2CNTL   ** ACTUAL REPORTING OUT TOTALS                           
 OUTFIL FNAMES=SUMSYSTM,NODETAIL,                                               
 HEADER2=(' DATE: ',DATE,4X,'PAGE: ',PAGE,2/,                                   
 10:'ALPHA',16:'SYS',                                                           
 40:'ORDERD',57:'PAID',73:'BILLED',89:'ASSIGNED',                               
