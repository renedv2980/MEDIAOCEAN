*                                                                               
* BUILD HEADER RECORD FOR EACH SYSTEM.                                          
*                                                                               
./ ADD NAME=HDR1CNTL   ** CREATE HEADER OUTPUT CONTROL CARDS                    
 OPTION COPY,NOCHALT,STOPAFT=1                                                  
 INREC  OVERLAY=(A9RUNDTE:A9RUNDTE,TRAN=ALTSEQ,                                 
                 A9STRDTE:A9STRDTE,TRAN=ALTSEQ,                                 
                 A9ENDDTE:A9ENDDTE,TRAN=ALTSEQ)                                 
 ALTSEQ CODE=(FAF0,FBF1,FCF2,FDF3,FEF4,FF00)                                    
 OUTREC BUILD=(C'HDR',DELIMITER,                                                
               A9ID,DELIMITER,                                                  
               A9SYSTEM,DELIMITER,                                              
               A9RUNDTE,Y2T,EDIT=(TTTT-TT-TT),LENGTH=10,DELIMITER,              
               A9STRDTE,Y2T,EDIT=(TTTT-TT-TT),LENGTH=10,DELIMITER,              
               A9ENDDTE,Y2T,EDIT=(TTTT-TT-TT),LENGTH=10,DELIMITER,              
               A9$TYPE,DELIMITER,                                               
               A9OPT1,DELIMITER,                                                
               A9OPT2)                                                          
 OUTFIL FTOV,FNAMES=VBOUTFIL                                                    
